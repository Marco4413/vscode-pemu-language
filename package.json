{
    "publisher": "hds",
    "name": "pemu-language-extension",
    "displayName": "PEMU Language Extension",
    "description": "An Extension which adds support for the PEMU Programming Language",
    "version": "0.0.6",
    "homepage": "https://github.com/hds536jhmk/vscode-pemu-language",
    "license": "MIT",
    "icon": "icon.png",
    "author": {
        "name": "hds536jhmk",
        "url": "https://github.com/hds536jhmk"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/hds536jhmk/vscode-pemu-language.git"
    },
    "engines": {
        "vscode": "^1.58.0"
    },
    "categories": [
        "Programming Languages", "Snippets"
    ],
    "keywords": [
        "pemu", "pemu language"
    ],
    "contributes": {
        "languages": [{
            "id": "pemu",
            "aliases": ["PEMU", "pemu"],
            "extensions": [".pemu",".pemulib"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "pemu",
            "scopeName": "source.pemu",
            "path": "./syntaxes/pemu.tmLanguage.json"
        }],
        "snippets": [{
                "language": "pemu",
                "path": "./snippets/pemu.code-snippets"
        }],
        "iconThemes": [{
            "id": "pemu",
            "label": "PEMU",
            "path": "./fileicons/pemu-icon-theme.json"
        }]
    }
}